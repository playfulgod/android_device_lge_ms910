<?xml version="1.0" encoding="utf-8"?>
<mms_config version="4">
    <bool name="enabledMMS">true</bool>
    <bool name="enabledTransID">true</bool>
    <int name="maxMessageSize">512000</int>
    <int name="maxImageHeight">960</int>
    <int name="maxImageWidth">1280</int>
    <int name="maxVideoHeight">240</int>
    <int name="maxVideoWidth">320</int>
    <!-- If true, The text message over 160 characters will be sent in multi part.
         If false, The text message over 160 characters will be sent
         via multi media message. -->
    <bool name="enableMultipartSMS">false</bool>

    <!-- If true, the text message will be split every 160 characters.
         If false, the text will never be split before being sent. -->
    <bool name="enableSplitSMS">true</bool>

    <string name="uaProfTagName">Profile</string>
    <string name="userAgent">LG-MS910/Android-mms\u0020</string>
    <string name="uaProfUrl">http://uaprof.metropcs.net/UAProf/LG-MS910.xml</string>
    <!-- Setting for MetroPCS MMS -->
    <string name="httpParams">x-up-calling-line-id: 1##LINE1##|X-VzW-MDN: 1##LINE1##</string>

    <!-- Setting for MetroPCS MMS -->
    <string name="httpParamsLine1Key">##LINE1##</string>
</mms_config>
